{
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    },
    "cSpell.userWords": [
        "autoescape",
        "autoload",
        "autowiring",
        "brandnew",
        "chriscodes",
        "chrisrobles",
        "Classname",
        "codepen",
        "coderay",
        "disqus",
        "doesnt",
        "dont",
        "dribbble",
        "endapply",
        "endblocks",
        "endcache",
        "endcapture",
        "endfor",
        "endmacro",
        "endsandbox",
        "endset",
        "endverbatim",
        "endwith",
        "flickr",
        "frontmatter",
        "hackernews",
        "head",
        "hydecorp",
        "importmap",
        "keybase",
        "mathjax",
        "overriden",
        "PHPSESSID",
        "pinterest",
        "preconfigured",
        "revindex",
        "rootform",
        "sensio",
        "shouldnt",
        "soundcloud",
        "sprintf",
        "stackoverflow",
        "striptags",
        "Swiftmailer",
        "symfony",
        "taylantatli",
        "varargs",
        "WASD",
        "weibo",
        "xmpp"
    ],
    "markdown-preview-github-styles.colorTheme": "dark",
    "markdownlint.config": {
        "blanks-around-fences": true,
        "MD031": false,
        "MD013": false,
        "MD047": false,
        "MD026": false,
        "MD012": false,
        "MD032": false,
        "MD036": false,
        "MD025": false,
        "MD033": false,
        "MD009": false,
        "MD010": false,
    },
    "markdownlint.focusMode": 2,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "editor.minimap.enabled": false,
    "markdown-preview-enhanced.scrollSync": false,
    "terminal.integrated.defaultProfile.osx": "bash",
    "editor.wordWrap": "off",
    "html.autoCreateQuotes": false,
    "editor.acceptSuggestionOnEnter": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "markdown.preview.doubleClickToSwitchToEditor": false,
    //"editor.autoClosingQuotes": "never",
    "[markdown]": {
        "editor.autoClosingQuotes": "never",
    },
}